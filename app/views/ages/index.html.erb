<!-- <h1>Listing ages</h1> -->
<h1><%= link_to 'What Would You Do If You Were 18 Again?', new_age_path %></h1>

<table>
  <tr>
        <!-- <th>Question</th> -->
        <th>Answer</th>
        <th></th>
        <th></th>
        <th></th>
      </tr>

<% @ages.each do |age| %>
<ol>
  <tr>
    <td><%= age.question %></td>
    <td><%= age.answer %></td>
    <td><%= link_to 'Show', age %></td>
    <td><%= link_to 'Edit', edit_age_path(age) %></td>
    <td><%= link_to 'Destroy', age, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

